            "homepage": "http://hiqdev.com/"
        }
    ],
    "require": {
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "~1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\composerextensionplugin\\": "src"
        }
    },
    "replace": {
        "yiisoft/yii2-composer": "*"
    },
    "extra": {
        "class": "hiqdev\\composerextensionplugin\\Plugin"
    }
}
